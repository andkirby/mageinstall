#!/bin/sh

# ======= Not desirable to change ========

# Set project domain
if [ ! "$PROJECT_DOMAIN" ]
then
    PROJECT_DOMAIN=`echo $PROJECT_DOMAIN_MASK | sed 's/%HOST%/'$PROJECT'/'`
fi

# Save current path to variable
CUR_DIR=$(pwd)

# Set default DB host
if [ ! "$DB_HOST" ]
then
    DB_HOST="localhost"
fi

# Set Database name
DB_NAME=$(echo $DB_NAME | tr /.- _)
